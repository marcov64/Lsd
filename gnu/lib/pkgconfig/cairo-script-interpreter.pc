prefix=/mingw64
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo-script-interpreter
Description: script surface backend for cairo graphics library
Version: 1.18.0
Requires: cairo
Requires.private: zlib, libpng, fontconfig >=  2.2.95, freetype2 >=  9.7.3, pixman-1 >=  0.36.0, lzo2
Libs: -L${libdir} -lcairo-script-interpreter
Libs.private: -lm -lgdi32 -lmsimg32 -ldwrite -ld2d1 -lwindowscodecs
Cflags: -I${includedir}/cairo
