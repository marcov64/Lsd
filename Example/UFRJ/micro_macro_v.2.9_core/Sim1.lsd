#include "fun_head.h"

MODELBEGIN



/************************************  PRODUO PROGRAMADA  ***************************************/

if(!strcmp(label, "EXP_SALES") )
{
/*	
Firm variable
As vendas previstas so calculadas a partir da mdia das encomendas efetivas dos dois ltimos perodos, sobre a qual  aplicada uma taxa de crescimento esperada. Esta taxa de crescimento  obtida com base na mdia das encomendas efetivas dos dois ltimos perodos e dos dois perodos anteriores a estes, aplicando-se um fator de extrapolao.
*/
  
   v[0]=p->cal("EFOR", 1);
   v[1]=p->cal("EFOR", 2);
   v[2]=p->cal("EFOR", 3);
   v[3]=p->cal("EFOR", 4);
  	v[5]=p->cal("GAMA",0); //GAMA(<=1)PROJEO: REFLETE O ESTADO DAS EXPECTATIVAS E CONDIES DE CONCORRNCIA
   v[8]=(v[2]+v[3])/2;
   v[9]=(v[0]+v[1])/2;
    	if(v[8]!=0)
   		{
   		v[6]=v[9]*(1+v[5]*((v[9]-v[8])/v[8]));// extrapolao da taxa de crescimento
   		v[7]=max(0,v[6]);
   		}
   		else
   		{
   		v[7]=v[9];
   		} 
		res=v[7];
		goto end;
}

if(!strcmp(label,"PP"))
{
/*
Firm variable
A produo programada  calculada a partir das vendas previstas para o perodo, considerando a proporo das vendas que se deseja manter como estoque e descontando o estoque acumulado do perodo anterior.  Este valor estar sujeito  restrio imposta pela capacidade produtiva existente, tendo em vista a possibilidade de expandir a produo, atravs, por exemplo, do uso de hora-extra de trabalho.

A produo programada do setor de bens de capital  definida em funo da demanda observada no perodo  este setor produz sob encomenda.  Tambm neste caso, a produo estar sujeita  restrio imposta pela capacidade produtiva existente, considerando-se a possibilidade do uso hora-extra.  
*/
v[0]=p->cal("Ibk",0);
v[1]=p->cal("EXP_SALES", 0); 
v[2]=p->cal("CP",1);
v[3]=p->cal("BETA",0); //BETA >=1,margem de sobre-utilizao da capacidade produtiva
v[4]=v[3]*v[2];
	if(v[0]==0)// se o setor no  produtor de BK
		{
			v[5]=p->cal("SIGMA",0);//proporo de estoque desejada
			v[8]=p->cal("STOCK",1);//
			v[6]=v[1]*(1+v[5])-v[8];
			v[7]=max(0, min( v[4], v[6] ));
			
		}
	if(v[0]==1)// se o setor  produtor de BK
		{
		v[6]=p->cal("EFOR_BK_firm",0);
		v[7]=max(0,min(v[4],v[6]));
		}
v[9]=max(v[7],0);
res=v[9];
goto end;
}


/**************************************  DEMANDA POR BENS INTERMEDIRIOS  ************************************/


if(!strcmp(label,"UA"))
{
/*
A utilizao de insumos necessria para atender  produo programada  calculada atravs do produto da produo programada pelo coeficiente tcnico.
*/
	v[0]=p->cal("PP",0);
	v[1]=p->cal("A",0) ;
	v[2]=p->cal("A_M",0) ;
	res=v[0]*(v[1]+v[2]);

goto end;
}



if(!strcmp(label,"A_at"))
{
/*
Esta varivel define a razo entre os insumos disponveis e o total de insumos necessrios de cada setor de bens intermedirios para se realizar a produo programada.  
*/
	v[2]=p->cal("UA",0);
	v[3]=p->cal("DA_stock_expost",1);

		if(v[2]>0)
		v[0]=v[3]/v[2];
		
		if(v[2]==0)
		v[0]=1;
		
res=min(1,v[0]);
goto end;
}


if(!strcmp(label,"minA_at"))
{
/*
firm variable
A varivel minA_at define para cada setor a menor razo entre os insumos disponveis e os insumos necessrios para realizar a produo programada.
*/
for(v[1]=1,cur=p->search("Inputs"); cur!=NULL; cur=go_brother(cur) )
{
  v[2]=cur->cal("A_at",0);
  v[3]=min(v[2],1);
  v[1]=min(v[1],v[3]);
}

res=v[1];

goto end;
}


if(!strcmp(label,"DA_stock_exante"))
{
/*
O estoque de insumos de cada setor, antes da demanda por insumos para serem utilizados no perodo seguinte,  determinado subtraindo-se do total de insumos disponveis a quantidade de insumos que ser efetivamente usada na produo.  Este ltimo fator  calculado atravs do produto dos insumos necessrios a produo programada pelo valor da menor razo entre os insumos disponveis e os insumos necessrios.
*/
v[0]=p->cal("DA_stock_expost",1);
v[2]=p->cal("minA_at",0);
v[3]=p->cal("UA",0);
v[4]=v[0]-(v[3]*v[2]);
res=max(v[4],0);
goto end;
}



if(!strcmp(label, "DA") )
{
/*
A demanda por insumos de cada setor de bens intermedirios, que devero ser produzidos durante o perodo corrente para serem usados no perodo seguinte,  calculada com base na quantidade de insumos necessria para realizar a produo programada, sobre a qual  aplicada uma taxa de crescimento esperada e descontado o estoque restante.  A taxa de crescimento  definida pela mdia das encomendas efetivas dos dois ltimos perodos e dos dois perodos anteriores a estes, utilizando-se um fator de extrapolao. 

Os setores de bens intermedirios demandam exatamente a quantidade que iro precisar para produzir no perodo seguinte, sem descontar o estoque inicial, que  desconhecido, uma vez que no sua produo no perodo ser funo da encomenda de insumos feita por todos os setores e no de sua produo programada.   
*/

  
v[0]=p->cal("PP",0);	
v[1]=p->cal("A",0) ;
v[2]=p->cal("Ibi",0);
v[22]=p->cal("Ibk",0);
v[3]=p->cal("DA_stock_exante",0);

v[10]=p->cal("EFOR", 1);
v[11]=p->cal("EFOR", 2);
v[12]=p->cal("EFOR", 3);
v[13]=p->cal("EFOR", 4);
v[15]=p->cal("GAMA",0); //GAMA(<=1)PROJEO REFLETE O ESTADO DAS EXPECTATIVAS E CONDIES DE CONCORRNCIA
v[8]=(v[12]+v[13]);
v[9]=(v[10]+v[11]);

   if(v[8]!=0)
   v[16]=1+(v[15]*((v[9]-v[8])/v[8]));// extrapolao da taxa de crescimento
   else
   v[16]=1;
   	
          if(v[2]==0)//Se no for bem intermedirio
          {
               if(v[22]==1)
               {
               v[23]=p->cal("SIGMA",0);
           				v[4]=(v[0]*(1+v[23])*v[1])*(v[16])-v[3];
               }
               else
               v[4]=(v[0]*v[1])*(v[16])-v[3];
  				}
          if(v[2]==1)//Se for bem intermedirio
          v[4]=(v[0]*v[1])*(v[16]);
  
  res=max(v[4],0);
  goto end;
}


if(!strcmp(label,"Dem_intermediate"))
{
/*
A encomenda efetiva de insumos recebida por cada setor de bens intermedirios  definida pela soma da demanda por seus insumos de todos os setores da economia mais as exportaes.  
*/
last_update--;//repeat the computation any time is requested
if(c==NULL)//Avoids to be computed when the system activates the equation
{
res=-1;
goto end;
}
else
{
v[0]=c->cal("Id1",0);
v[8]=c->cal("Ibi",0);
if(v[8]==1)
{
	for(v[2]=0,cur=p->search("Sector"); cur!=NULL; cur=go_brother(cur) )
  {
     		for(v[3]=0,cur1=cur->search("Firm"); cur1!=NULL; cur1=go_brother(cur1) )
     {
      cur2=cur1->search_var_cond("Id3",v[0], 0);
      v[4]=cur2->cal("DA",0);
      v[3]=v[3]+v[4];
     }
  v[2]=v[2]+v[3];
  }
  //c->write("DA_TOTP",v[2], 0); /* O parmetro DA_TOTP soma a demanda de todos os setores pelos insumos de cada setor intermedirio */
res=v[2];
}
else
res=0;
goto end;
}
}


if(!strcmp(label,"DA_TOT"))
{
/*
Not Included yet
*/
		
	v[1]=p->cal("DA_TOTP",0);
   res=v[1];
