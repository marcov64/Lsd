   v[9]=p->cal("CP_DEPRECIACAO",0);
   v[4]=(1/v[3])*v[0]-(v[1]-v[9]);
   v[8]=max(0,v[4]);
   cur=p->search_var_cond("Id3",2,0);//Repensar se houver mais de um setor de BK
   v[5]=cur->cal("B",0);
   v[11]=v[8]*v[5];
   modf(v[11],&v[12]);
   v[13]=v[12]/v[5];
   }
   else
   v[13]=0;

res=v[13];
goto end;
}




